package com.techwebdocs.third;
import static com.techwebdocs.third.QuickSort.printArray;

public class Third {

    public static void main(String[] args) {
        int arr[] = {55,45,16,66,99,43,14,68,23,46};        
        int n = arr.length;
        System.out.println("Given Array: "); 
        printArray(arr); 
        
        QuickSort ob = new QuickSort();
        ob.sort(arr, 0, n-1);
 
        System.out.println("sorted array is: ");
        printArray(arr);
    }
}
