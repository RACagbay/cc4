package com.techwebdocs.fourth;
public class Fourth {

    public static void main(String[] args) {
        BubbleSort ob = new BubbleSort(); 
        int arr[] = {55,45,16,66,99,43,14,68,23,46};       
        System.out.println("Given Array: "); 
        ob.printArray(arr);
        
        ob.bubbleSort(arr); 
        System.out.println("Sorted array"); 
        ob.printArray(arr); 
    }
}


